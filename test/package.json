    "typescript": "^2.1.6"
  },
  "dependencies": {
    "@types/selenium-webdriver": "^3.0.8",
    "@types/webdriverio": "^4.8.6",
    "selenium-webdriver": "^3.6.0",
    "webdriverio": "^4.8.0"
